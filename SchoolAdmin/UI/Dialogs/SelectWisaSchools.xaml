<Window x:Class="SchoolAdmin.UI.Dialogs.SelectWisaSchools"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SchoolAdmin.UI.Dialogs"
        mc:Ignorable="d"
        Title="Kies Werkdata" Height="450" Width="400">
    <Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="*"/>
			<RowDefinition Height="50"/>
		</Grid.RowDefinitions>
		<ScrollViewer Grid.Row="0" Margin="10">
			<StackPanel x:Name="Schools" Orientation="Vertical">

			</StackPanel>
		</ScrollViewer>
		<Button Grid.Row="1" x:Name="ReadData" Content="Download Data" Click="ReadData_Click" Margin="10"/>
	</Grid>
</Window>
